{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "noImplicitAny": false,
    "sourceMap": true,
    "noEmitOnError": true,
    "strict": true,
    "outDir": "./dist",
    "types": ["node", "mocha"],
    "typeRoots": ["node_modules/@types", "types"],
    "strictPropertyInitialization": false
  },
  "exclude": ["node_modules", "aws"]
}
