include space

mixin accent(url, text, width)
  table(width="100%" height=152 border="0" cellpadding="0" cellspacing="0")
    tr
      td(height=37)
    tr
      td(height=70)
        table.accentWrapper(width="100%" height="70" border="0" cellpadding="0" cellspacing="0")
          tr
            td
              +space()
            td.accent(width="260" height="70")&attributes(attributes)
              block
            td
              +space()
    tr
      td(height=45)
