{
  "name": "api.finex.io",
  "version": "1.3.0",
  "author": "Maksim Amelchenko <m.amelchenko@gmail.com>",
  "main": "server.js",
  "scripts": {
    "build": "echo $npm_package_version:$GIT_HASH && mkdir -p dist && printf $npm_package_version:$GIT_HASH > ./dist/version && cp package.json dist/ && cp package-lock.json dist/ && cp -a public dist/ && cp -a ./emails/src/ dist/emails && find ./migrations -name '*.sql' | cpio -pdm ./dist && ./node_modules/.bin/tsc",
    "migrate": "knex migrate:latest",
    "migrate:dev": "NODE_ENV=development-local knex migrate:latest",
    "start": "npm run migrate && node server.js",
    "start:dev": "NODE_ENV=development-local ts-node-dev -r dotenv/config --respawn --transpile-only server.ts dotenv_config_path=.env.dev | bunyan",
    "test": "NODE_ENV=development-test-local mocha ./**/*.spec.ts --require ts-node/register --exit"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "aws-sdk": "^2.1204.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.29.2",
    "email-templates": "^10.0.1",
    "got": "^11.8.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "knex": "^2.2.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-mount": "^4.0.0",
    "koa-response-time": "^2.1.0",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaults": "^4.2.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isobject": "^3.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nconf": "^0.12.0",
    "nodemailer": "^6.7.8",
    "numeral": "^2.0.6",
    "objection": "^3.0.1",
    "openapi-types": "^12.0.0",
    "pg": "^8.8.0",
    "pg-error-constants": "^1.0.0",
    "source-map-support": "^0.5.21",
    "stoppable": "^1.1.0",
    "swagger-ui-dist": "^4.14.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.8",
    "@types/chai": "^4.3.3",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-router": "^7.4.4",
    "@types/mocha": "^9.1.1",
    "@types/multer": "^1.4.7",
    "@types/nconf": "^0.10.3",
    "@types/node": "^16.11.56",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "mocha": "^10.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
