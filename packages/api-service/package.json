{
  "name": "api.finex.io",
  "author": "Maksim Amelchenko <m.amelchenko@gmail.com>",
  "main": "server.js",
  "scripts": {
    "build": "echo $npm_package_version:$GIT_HASH && mkdir -p dist && printf $npm_package_version:$GIT_HASH > ./dist/version && cp package.json dist/ && cp package-lock.json dist/ && cp -a public dist/ && cp -a ./emails/src/ dist/emails && find ./migrations -name '*.sql' | cpio -pdm ./dist && ./node_modules/.bin/tsc",
    "migrate": "knex migrate:latest",
    "migrate:dev": "NODE_ENV=development-local knex migrate:latest",
    "start": "npm run migrate && node server.js",
    "start:dev": "NODE_ENV=development-local ts-node-dev -r dotenv/config --respawn --transpile-only server.ts | bunyan",
    "test": "NODE_ENV=development-test-local mocha ./tests/*.ts --require ts-node/register"
  },
  "dependencies": {
    "ajv": "^8.0.5",
    "ajv-formats": "^2.0.2",
    "aws-sdk": "^2.881.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "email-templates": "^8.0.4",
    "got": "^11.8.2",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "knex": "^0.21.19",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-mount": "^4.0.0",
    "koa-response-time": "^2.1.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaults": "^4.2.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isobject": "^3.0.2",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "nconf": "^0.11.2",
    "nodemailer": "^6.5.0",
    "openapi-types": "^7.2.3",
    "pg": "^8.5.1",
    "source-map-support": "^0.5.19",
    "stoppable": "^1.1.0",
    "swagger-ui-dist": "^3.46.0",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.6",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/mocha": "^8.2.2",
    "@types/multer": "^1.4.5",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.14.37",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@types/ua-parser-js": "^0.7.35",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.3.2",
    "should": "^13.2.3",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
